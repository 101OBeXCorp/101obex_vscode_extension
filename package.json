{
  "name": "101obex-api-extension-api-publish",
  "displayName": "AVAP API Release Manager",
  "description": "AVAP API Release Manager",
  "version": "1.30.135",
  "publisher": "101OBeXCorp",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "extensionPack": [],
  "extensionDependencies": [
    "101OBeXCorp.101obex-api-extension"
  ],
  "recommendations": [
    "101OBeXCorp.101obex-api-extension"
  ],
  "icon": "images/AVAP_APIReleaseManager.png",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "101obex-api-publish",
          "title": "AVAP API Release Manager",
          "icon": "images/icono_avap.png"
        }
      ]
    },
    "views": {
      "101obex-api-extension": [
        {
          "id": "101obex-api-publish.package-publisher",
          "name": "AVAP API Release Manager",
          "type": "tree"
        }
      ]
    },
    "commands": [
      {
        "command": "101obex-api-extension-api-publisher.refreshEntry-api-publisher",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "101obex-api-extension-api-publisher.addAPI-api-publisher",
        "title": "add API Product",
        "icon": "$(plus)"
      },
      {
        "command": "101obex-api-extension-api-publisher.addAPI-api-publisher-nodata",
        "title": "add API Product",
        "icon": "$(plus)"
      },
      {
        "command": "101obex-api-extension-api-publisher.linkAPI-api-publisher",
        "title": "Link External API",
        "icon": "$(link)"
      },
      {
        "command": "101obex-api-extension-api-publisher.commitPush-api-publisher",
        "title": "commit push",
        "icon": "$(repo-push)"
      },
      {
        "command": "101obex-api-extension-api-publisher.checkout-api-publisher",
        "title": "commit checkout",
        "icon": "$(repo-push)"
      },
      {
        "command": "101obex-api-extension-api-publisher.eraseAPI",
        "title": "Erase API",
        "icon": {
          "light": "images/bin.svg",
          "dark": "images/bin-white.svg"
        }
      },

      {
        "command": "101obex-api-extension-api-publisher.CloneAPI",
        "title": "Publish API",
        "icon": "$(megaphone)"
      },
      {
        "command": "101obex-api-extension-api-publisher.PublishAPIProd",
        "title": "Publish API Product",
        "icon": "images/Logo_Marketplace.svg"
      },

      {
        "command": "101obex-api-extension-api-publisher.CheckoutAPI",
        "title": "Checkout this Commit"
      },

      {
        "command": "101obex-api-extension-api-publisher.TestThisAPI",
        "title": "Test this API",
        "icon": "$(testing-run-icon)"
      },

      {
        "command": "101obex-api-extension-api-publisher.CopyThisAPI",
        "title": "Copy this API request",
        "icon": "$(files)"
      },

      {
        "command": "101obex-api-extension-api-publisher.GetLink",
        "title": "Get API Link"
      },

      {
        "command": "101obex-api-extension-api-publisher.ConfigTests",
        "title": "Unit Test",
         "icon": "$(beaker)"
      }

      ,

      {
        "command": "101obex-api-extension-api-publisher.ConfigDocs",
        "title": "Unit Test",
         "icon": "$(remote-explorer-documentation)"
      }

    ],
    "menus": {
      "view/title": [
        {
          "command": "101obex-api-extension-api-publisher.refreshEntry-api-publisher",
          "when": "view == 101obex-api-publish.package-publisher",
          "group": "navigation"
        },
        {
          "command": "101obex-api-extension-api-publisher.addAPI-api-publisher",
          "when": "view == 101obex-api-publish.package-publisher",
          "group": "navigation"
        }
        ,
        {
          "command": "101obex-api-extension-api-publisher.linkAPI-api-publisher",
          "when": "view == 101obex-api-publish.package-publisher",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "101obex-api-extension-api-publisher.CloneAPI",
          "when": "view == 101obex-api-publish.package-publisher && viewItem == APICONF"
        },        
        {
          "command": "101obex-api-extension-api-publisher.ConfigTests",
          "when": "view == 101obex-api-publish.package-publisher && viewItem == API_CONF",
          "group": "inline"
        },
        {
          "command": "101obex-api-extension-api-publisher.ConfigDocs",
          "when": "view == 101obex-api-publish.package-publisher && viewItem == API_CONF",
          "group": "inline"
        },
        {
          "command": "101obex-api-extension-api-publisher.TestThisAPI",
          "when": "view == 101obex-api-publish.package-publisher && viewItem == API_Published",
          "group": "inline"
        },
        {
          "command": "101obex-api-extension-api-publisher.CopyThisAPI",
          "when": "view == 101obex-api-publish.package-publisher && viewItem == API_Published",
          "group": "inline"
        },
        {
          "command": "101obex-api-extension-api-publisher.CloneAPI",
          "when": "view == 101obex-api-publish.package-publisher && viewItem == APICONF",
          "group": "inline"
        },
        {
          "command": "101obex-api-extension-api-publisher.eraseAPI",
          "when": "view == 101obex-api-publish.package-publisher && viewItem == API_NOT_PUBLISHED",
          "group": "inline"
        },
        {
          "command": "101obex-api-extension-api-publisher.CheckoutAPI",
          "when": "view == 101obex-api-publish.package-publisher && viewItem == COMMITCONF"
        },
        {
          "command": "101obex-api-extension-api-publisher.TestThisAPI",
          "when": "view == 101obex-api-publish.package-publisher && viewItem == API_Published"
        },

        {
          "command": "101obex-api-extension-api-publisher.ConfigTests",
          "when": "view == 101obex-api-publish.package-publisher && viewItem == API_CONF"
        }
        ,

        {
          "command": "101obex-api-extension-api-publisher.ConfigDocs",
          "when": "view == 101obex-api-publish.package-publisher && viewItem == API_CONF"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/request": "^2.48.8",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "axios": "^1.2.0",
    "chatgpt": "^5.1.2",
    "expiry-map": "^2.0.0",
    "gpt3-tokenizer": "^1.1.5",
    "node-datetime": "^2.1.2",
    "node-fetch": "^3.3.1",
    "request": "^2.88.2",
    "requests": "^0.3.0",
    "vsce": "^2.15.0",
    "vscode-uri": "^3.0.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/101OBeXCorp/101obex_vscode_extension.git"
  }
}
